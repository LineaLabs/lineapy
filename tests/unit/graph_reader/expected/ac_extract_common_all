def get_a_for_artifact_b_and_downstream():
    a = 1
    a += 1
    return a


def get_b(a):
    b = a + 1
    return b


def get_c(a):
    c = a + 2
    return c


def run_session_including_b():
    # Given multiple artifacts, we need to save each right after
    # its calculation to protect from any irrelevant downstream
    # mutations (e.g., inside other artifact calculations)
    import copy

    artifacts = dict()
    a = get_a_for_artifact_b_and_downstream()
    b = get_b(a)
    artifacts["b"] = copy.deepcopy(b)
    c = get_c(a)
    artifacts["c"] = copy.deepcopy(c)
    return artifacts


def run_all_sessions():
    artifacts = dict()
    artifacts.update(run_session_including_b())
    return artifacts


if __name__ == "__main__":
    run_all_sessions()
