def get_a0():
    a0 = 0
    a0 += 1
    return a0


def get_a_for_artifact_c_and_downstream():
    a = 1
    a += 1
    return a


def get_c(a, a0):
    b = a * 2 + a0
    c = b + 3
    return c


def get_h(a, c):
    d = a * 4
    e = d + 5
    e += 6
    a += 1
    g = c + e * 2
    h = a + g
    return h


def run_session_including_a0():
    # Given multiple artifacts, we need to save each right after
    # its calculation to protect from any irrelevant downstream
    # mutations (e.g., inside other artifact calculations)
    import copy

    artifacts = []
    a0 = get_a0()
    artifacts.append(copy.deepcopy(a0))
    a = get_a_for_artifact_c_and_downstream()
    c = get_c(a, a0)
    artifacts.append(copy.deepcopy(c))
    h = get_h(a, c)
    artifacts.append(copy.deepcopy(h))
    return artifacts


def run_all_sessions():
    a0, c, h = run_session_including_a0()
    return a0, c, h


if __name__ == "__main__":
    run_all_sessions()
