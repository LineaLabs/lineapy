def get_a0():
    a0 = 0
    a0 += 1
    return a0


def get_b0():
    b0 = 0
    return b0


def run_session_including_a0():
    a0 = get_a0()
    return a0


def run_session_including_b0():
    b0 = get_b0()
    return b0


def run_all_sessions():
    a0 = run_session_including_a0()
    b0 = run_session_including_b0()
    return a0, b0


if __name__ == "__main__":
    run_all_sessions()
