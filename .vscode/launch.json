    "version": "0.2.0",
    "configurations": [
        {
            "name": "lineapy --slice p value",
            "type": "python",
            "request": "launch",
            "module": "lineapy.cli.cli",
            "args": [
                "--slice",
                "p value",
                "tests/housing.py",
            ],
        },
        {
            "name": "lineapy --slice p value --slice y",
            "type": "python",
            "request": "launch",
            "module": "lineapy.cli.cli",
            "args": [
                "--slice",
                "p value",
                "--slice",
                "y",
                "tests/housing.py",
            ],
        },
        {
            "name": "lineapy --export-slice",
            "type": "python",
            "request": "launch",
            "module": "lineapy.cli.cli",
            "args": [
                "--slice",
                "p value",
                "--export-slice",
                "p_value_housing",
                "--slice",
                "y",
                "--export-slice",
                "y_housing",
                "tests/housing.py",
            ],
        },
        {
            "name": "lineapy --airflow",
            "type": "python",
            "request": "launch",
            "module": "lineapy.cli.cli",
            "args": [
                "--slice",
                "p value",
                "--slice",
                "y",
                "--airflow",
                "sliced_housing_dag",
                "tests/housing.py",
            ],
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}